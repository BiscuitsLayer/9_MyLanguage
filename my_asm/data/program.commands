DEF_CMD (PUSHRM_CX, 9, PUSH(RAM(CX)))

DEF_CMD (PUSHRM_DX, 10, PUSH(RAM(DX)))

DEF_CMD (POP, 20, POP)

DEF_CMD (POPRM, 21, POP)

DEF_CMD (POP_AX, 22, AX = POP)

DEF_CMD (POP_BX, 23, BX = POP)

DEF_CMD (POP_CX, 24, CX = POP)

DEF_CMD (POP_DX, 25, DX = POP)

