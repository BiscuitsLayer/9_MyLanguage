DEF_CMD (PUSHRM, 2, PUSH(RAM(ARG(1))))

DEF_CMD (PUSH_AX, 3, PUSH(AX))

DEF_CMD (PUSH_BX, 4, PUSH(BX))

DEF_CMD (PUSH_CX, 5, PUSH(CX))

DEF_CMD (PUSH_DX, 6, PUSH(DX))

DEF_CMD (PUSHRM_AX, 7, PUSH(RAM(AX)))

