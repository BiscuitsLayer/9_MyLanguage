DEF_CMD (JB, 43, IF((-1.0) * (POP - POP) < 0, GOTO(ARG(1))))

DEF_CMD (JBE, 44, IF((-1.0) * (POP - POP) <= 0, GOTO(ARG(1))))

DEF_CMD (JE, 45, IF((-1.0) * (POP - POP) == 0, GOTO(ARG(1))))

DEF_CMD (JNE, 46, IF((-1.0) * (POP - POP) != 0, GOTO(ARG(1))))

DEF_CMD (CALL, 50, DO(SAVELOC, GOTO(ARG(1))))

